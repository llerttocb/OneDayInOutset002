<Window x:Class="OneDayInOutset002.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OneDayInOutset002"
        mc:Ignorable="d"
        Title="One Day in Outset"
        Height="450"
        Width="800"
        Background="Black"
        >
    <Grid>
        <TextBlock
            Name="MainText"
            FontSize="12"
            TextAlignment="Left"
            Foreground="White"
            Margin="20,20,20,79"
            >
        </TextBlock>
        <Button
            Name="Button1"
            Height="40"
            Width="100"
            Background="Gray"
            Margin="20,359,672,20"
            Click="ButtonClick"
            >
        </Button>
        <Button
            Name="ButtonSkip"
            Height="40"
            Width="100"
            Background="Gray"
            Margin="145,360,547,19"
            Click="Skip"
            Content="Skip"
            >
        </Button>
        <TextBlock
            Name="Continue"
            Height="40"
            Width="400"
            Margin="372,360,20,19"
            Foreground="White"
            >
            
        </TextBlock>
    </Grid>
</Window>
